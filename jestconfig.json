{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testTimeout": 10000,
  "testEnvironment": "node",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "coverageDirectory": "./coverage/",
  "collectCoverage": true
}